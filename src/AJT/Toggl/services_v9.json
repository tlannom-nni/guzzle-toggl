    "CreateClient": {
      "httpMethod": "POST",
      "uri": "workspaces/{workspace_id}/clients",
      "summary": "Create Client",
      "responseModel": "getResponse",
      "parameters": {
        "workspace_id": {
          "location": "uri",
          "type": "integer",
          "required": true,
          "description": "Numeric ID of the workspace"
        },
        "client": {
          "location": "json",
          "type": "object",
          "description": "Client Object",
          "properties": {
            "name": {
              "type": "string",
              "required": true,
              "description": " The name of the client (string, required, unique in workspace)"
            },
            "wid": {
              "type": "integer",
              "required": true,
              "description": "workspace ID, where the client will be used (integer, required)"
            }
          }
        }
      }
    },
